mport numpy as np

# Define matrices A and B
A = np.array([[4, 7],
              [2, 6]])

B = np.array([[1, 2],
              [3, 4]])

# Compute inverse of B
B_inv = np.linalg.inv(B)

# Multiply A by inverse of B
result = np.dot(A, B_inv)

print("Result of A divided by B (A * B^-1):")
print(result)
